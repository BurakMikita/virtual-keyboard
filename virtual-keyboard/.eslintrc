{   

	"env": {
    "browser": true,
	 "es6": true,
	 "node": true
	},
	"extends": ["airbnb/base","prettier"],
	"plugins": ["prettier"],
	"rules": {
		"no-console": "off",
		"no-plusplus":"off",
		"no-tabs":"off",
		"semi":"off",
		"no-multiple-empty-lines":"off",
		"indent":"off",
		 "camelcase":"off",
		 "spaced-comment":"off",
		 "no-shadow":"off",
		 "no-param-reassign":"off"
      
		 
	},
	"parserOptions": {
		"ecmaVersion": 13,
		"sourceType": "module"
		}
	
}